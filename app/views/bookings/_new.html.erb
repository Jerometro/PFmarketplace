<%= simple_form_for [ @laptop, @booking ] do |f| %>
  <%= f.input :start_date, as: :datetime, html5: true %>
  <%= f.input :end_date, as: :datetime, html5: true %>
  <%= f.submit "Submit Booking", class: "btn btn-success" %>
  <%= link_to "Back to laptops", laptops_path(@laptop), class: "btn btn-warning" %>
<% end %>
